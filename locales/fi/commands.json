{
    "ping": {
        "name": "ping",
        "description": "Check if the bot is alive"
    },
    "serverinfo": {
        "name": "serverinfo",
        "description": "Get information about this guild",
        "embed": {
            "author": "{{guild_name}} - information",
            "serverinformation": {
                "name": "Server Information:",
                "value1": "**ID:** `{{guild_id}}`",
                "value2": "**Owner:** ",
                "value3": "**Created:** "
            },
            "members": {
                "name": "Members:",
                "value1": "**Members:** `{{guild_totalMembers}}`",
                "value2": "**Humans:** `{{guild_totalHumanMembers}}`",
                "value3": "**Bots:** `{{guild_bots}}`"
            },
            "newMembers": {
                "name": "**New Members:**",
                "value1": "**Last 12 Months:** `{{guild_lastMembers12Months}}`",
                "value2": "**Last 6 Months:** `{{guild_lastMembers6Months}}`",
                "value3": "**Last 30 Days:** `{{guild_lastMembers30Days}}`",
                "value4": "**Last 7 Days:** `{{guild_lastMembers7days}}`",
                "value5": "**Last 24 Hours:** `{{guild_lastMembers24hrs}}`"
            }
        }
    },
    "sync": {
        "name": "sync",
        "description": "Synchronize any of our modules to work on your server.",
        "verification": {
            "name": "verification",
            "description": "Synchronize the verification system."
        }
    }
}
